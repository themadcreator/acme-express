{
  "name"        : "acme-express",
  "version"     : "0.0.0",
  "description" : "ACME protocol client for SSL certificates",
  "bin"         : {
    "acme-express" : "bin/acme-express"
  },
  "main"    : "src/api.coffee",
  "scripts" : {
    "run"        : "./bin/acme-express",
    "deploy"     : "npm version minor -m \"Update version to %s\" ",
    "postdeploy" : "git push && git push --tags"
  },
  "keywords": [
    "acme",
    "letsencrypt",
    "https",
    "ssl",
    "certificate"
  ],
  "author"       : "themadcreator@github",
  "license"      : "Apache-2.0",
  "dependencies" : {
    "bluebird"      : "3.1.1",
    "chalk"         : "1.1.1",
    "coffee-script" : "1.10.0",
    "commander"     : "2.9.0",
    "request"       : "2.67.0",
    "ursa"          : "0.9.1"
  }
}
